# Read in data
data = load ('data.txt');
x = data(:,4);
y = data(:,5);
z = data(:,6);

# Create Sub matrix of first 10 values
x_small = x(1:10,:);
y_small = y(1:10,:);
z_small = z(1:10,:);

# Print mean of sub matrices
printf("Mean x: %f, y: %f, z: %f\r\n", mean(x_small), mean(y_small), mean(z_small));