/*
 * gpio.c: Buttons, LEDs, etc.
 * Author: Stephanie Kunkel
 */

/*
 * Includes
 */
#include "gpio.h"

/*
 * Global Variables
 */
XGpio Gpio;

/*
 * Init
 */
void initGpio(){
	//Variables
	XGpio_Config gpioConfig;

	XGpio_CfgInitialize();
	XGpio_CfgInitialize(&Gpio, &gpioConfig, GPIO_ADDRESS);
	XGpio_Initialize(&Gpio, GPIO_DEVICE_ID);
}
