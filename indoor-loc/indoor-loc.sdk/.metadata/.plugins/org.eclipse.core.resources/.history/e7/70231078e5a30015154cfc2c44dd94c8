/*
 * Convert Timestamp to s
 */
float timestampToSec(long timestamp) {
	return timestamp / COUNTS_PER_US;
}

/*
 * Convert Timestamp to ms
 */

/*
 * Convert Timestamp to us
 */
float timestampToSec(long timestamp) {
	return timestamp / COUNTS_PER_US;
}
